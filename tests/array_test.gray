let test_array = [];

array::push(test_array, 50);
array::push(test_array, 25);
array::push(test_array, 100);

assert_eq(array::get(test_array, 0), 50);
assert_eq(array::get(test_array, 1), 25);
assert_eq(array::get(test_array, 2), 100);

let test_array = [ 50, 25, 100 ];
assert_eq(array::get(test_array, 0), 50);
assert_eq(array::get(test_array, 1), 25);
assert_eq(array::get(test_array, 2), 100);

assert_eq(array::len(test_array), 3);


let array = [50, 20, 100];
assert_eq(array[1], 20);
array[1] = 25;
assert_eq(array[1], 25);


let obj = {
    a: [50, 20, 100],
};

assert_eq(obj.a[1], 20);
obj.a[1] = 25;
assert_eq(obj.a[1], 25);

let obj = {
    array: [[-10, [0, -22]], 20, 2],
};

obj.array[0][1][1] = 15;

assert_eq(obj.array[0][1][1], 15);