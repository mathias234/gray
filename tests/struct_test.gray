struct StructsWithMethodAndProperties {
    let A;
    let B;
    let C;

    fn new(self, a) {
        self.A = a;
        self.B = self.A * 2;
        self.C = self.B * 2;

        return self;
    }

    fn get_a(self) {
        return self.A;
    }
}


let a = 55;
let instance = StructsWithMethodAndProperties.new(a);

assert_eq(instance.get_a(), a);